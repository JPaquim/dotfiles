[user]
	email = joaopaquim@gmail.com
	name = jpaquim
[core]
	editor = vim
[push]
	default = current
[alias]
	br = branch
	brd = branch -d
	brD = branch -D
	ci = commit
	co = checkout
	cob = checkout -b
	pullr = pull --rebase origin master
	pushf = push --force-with-lease
	pushfn = push --force-with-lease --no-verify
	rb = rebase
	rbi = rebase -i
	rbc = rebase --continue
	rba = rebase --abort
	st = status
	temp = commit -m temp
[gpg]
	program = /usr/local/bin/gpg
[hub]
	protocol = https
